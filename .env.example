# Autonomous Vertex K.E.N. System Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
NEON_DATABASE_URL=postgresql://neondb_owner:password@endpoint.neon.tech/neondb?sslmode=require

# =============================================================================
# AI SERVICES
# =============================================================================
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here
GOOGLE_AI_API_KEY=AIza_your_google_ai_key_here

# =============================================================================
# INFRASTRUCTURE
# =============================================================================
HETZNER_TOKEN=your_hetzner_token_here
GITHUB_TOKEN=ghp_your_github_token_here
DOCKER_REGISTRY_URL=registry.example.com

# =============================================================================
# PAYMENT PROCESSING
# =============================================================================
STRIPE_API_KEY=sk_live_your_stripe_key_here
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# =============================================================================
# COMMUNICATION SERVICES
# =============================================================================
TWILIO_ACCOUNT_SID=AC_your_twilio_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
SENDGRID_API_KEY=SG.your_sendgrid_key_here
MAILCHIMP_API_KEY=your_mailchimp_key_here

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================
GOOGLE_ANALYTICS_ID=GA4-your_analytics_id_here
FACEBOOK_PIXEL_ID=your_facebook_pixel_id_here
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here
PROMETHEUS_CONFIG_PATH=/etc/prometheus/prometheus.yml

# =============================================================================
# AUTOMATION & WORKFLOW
# =============================================================================
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_n8n_password_here
ZAPIER_API_KEY=your_zapier_key_here

# =============================================================================
# AFFILIATE NETWORKS
# =============================================================================
SHAREASALE_API_KEY=your_shareasale_key_here
CJ_API_KEY=your_cj_key_here
IMPACT_API_KEY=your_impact_key_here
AMAZON_ASSOCIATES_TRACKING_ID=your_amazon_tracking_id_here

# =============================================================================
# SOCIAL MEDIA & MARKETING
# =============================================================================
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here

# =============================================================================
# WEB SERVICES & TOOLS
# =============================================================================
SPIDER_CLOUD_API_KEY=your_spider_cloud_key_here
BROWSERBASE_API_KEY=your_browserbase_key_here
PLAYWRIGHT_HEADLESS=true

# =============================================================================
# SECURITY & VAULT
# =============================================================================
SECRET_KEY=your_generated_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key_here
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=your_vault_token_here
ENCRYPTION_KEY=your_encryption_key_here

# =============================================================================
# K.E.N. SYSTEM SPECIFIC
# =============================================================================
KEN_ENHANCEMENT_TARGET=847329
KEN_COST_BUDGET_EUR=17.99
KEN_CACHE_LEVELS=4
KEN_ALGORITHM_COUNT=42
KEN_TRITON_ENABLED=true

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=info
PORT=3000
HOST=0.0.0.0

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0

# =============================================================================
# KUBERNETES CONFIGURATION
# =============================================================================
KUBECONFIG=/home/ubuntu/.kube/config
NAMESPACE=autonomous-vertex
CLUSTER_NAME=ken-cluster

# =============================================================================
# BACKUP & RECOVERY
# =============================================================================
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
S3_BACKUP_BUCKET=your-backup-bucket
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

