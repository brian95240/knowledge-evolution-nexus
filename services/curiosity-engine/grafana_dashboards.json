        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "üß† Enhancement Factor",
            "type": "stat",
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
            "targets": [
              {
                "expr": "ken_enhancement_factor_current",
                "legendFormat": "Enhancement Factor",
                "refId": "A"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {"mode": "palette-classic"},
                "custom": {
                  "displayMode": "basic",
                  "orientation": "horizontal"
                },
                "mappings": [],
                "thresholds": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "red", "value": 80}
                  ]
                },
                "unit": "scientific"
              }
            },
            "options": {
              "reduceOptions": {
                "values": false,
                "calcs": ["lastNotNull"],
                "fields": ""
              },
              "orientation": "auto",
              "textMode": "auto",
              "colorMode": "value",
              "graphMode": "area",
              "justifyMode": "auto"
            }
          },
          {
            "id": 2,
            "title": "üîç Discovery Rate (per minute)",
            "type": "graph",
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
            "targets": [
              {
                "expr": "rate(ken_curiosity_discoveries_total[5m]) * 60",
                "legendFormat": "Discoveries/min",
                "refId": "A"
              }
            ],
            "yAxes": [
              {
                "label": "Discoveries per minute",
                "min": 0
              }
            ],
            "xAxis": {
