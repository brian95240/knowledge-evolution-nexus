    {
      "dashboard": {
        "id": null,
        "title": "K.E.N. v3.1 Superior Bot Performance",
        "tags": ["ken", "bots", "performance", "api-comparison"],
        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "üì∫ YouTube Discovery Bot vs API",
            "type": "table",
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
            "targets": [
              {
                "expr": "ken_youtube_discovery_rate",
                "legendFormat": "K.E.N. Bot Rate",
                "refId": "A"
              }
            ],
            "transformations": [
              {
                "id": "organize",
                "options": {
                  "excludeByName": {},
                  "indexByName": {},
                  "renameByName": {
                    "Value": "Discoveries/min"
                  }
                }
              }
            ],
            "fieldConfig": {
              "defaults": {
                "custom": {
                  "displayMode": "color-background"
                },
                "mappings": [
                  {
                    "options": {
                      "from": 25,
                      "to": 60,
                      "result": {
                        "color": "green",
                        "text": "10x Faster than API"
                      }
                    },
                    "type": "range"
                  }
                ]
              }
            }
          },
          {
            "id": 2,
            "title": "üîç OCR Processing: K.E.N. vs Spider.cloud",
            "type": "stat",
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
            "targets": [
              {
                "expr": "ken_ocr_processing_accuracy",
                "legendFormat": "K.E.N. OCR Accuracy",
                "refId": "A"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "percent",
                "min": 0,
                "max": 100,
                "thresholds": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 89},
                    {"color": "green", "value": 94}
                  ]
                }
              }
            },
            "options": {
              "colorMode": "background",
              "graphMode": "area",
              "justifyMode": "center",
              "orientation": "horizontal",
              "reduceOptions": {
                "calcs": ["lastNotNull"],
                "fields": "",
                "values": false
              },
              "text": {
                "titleSize": 16,
                "valueSize": 24
              },
              "textMode": "value_and_name"
            }
          },
          {
            "id": 3,
            "title": "üí∞ Cost Comparison: K.E.N. vs External APIs",
            "type": "bargauge",
            "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
            "targets": [
              {
                "expr": "0",
                "legendFormat": "K.E.N. Bots Cost",
                "refId": "A"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "currencyUSD",
                "min": 0,
                "max": 1000,
                "thresholds": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 100},
                    {"color": "red", "value": 500}
                  ]
                }
              },
              "overrides": [
                {
                  "matcher": {"id": "byName", "options": "K.E.N. Bots Cost"},
                  "properties": [
                    {"id": "displayName", "value": "K.E.N. Bots: $0/month"},
                    {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
                  ]
                }
              ]
            },
            "options": {
              "reduceOptions": {
                "values": false,
                "calcs": ["lastNotNull"],
                "fields": ""
              },
              "orientation": "horizontal",
              "displayMode": "gradient",
              "showUnfilled": true
            }
          },
          {
            "id": 4,
            "title": "‚ö° Performance Metrics Comparison",
            "type": "table",
            "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
            "targets": [
              {
                "expr": "ken_bot_performance_comparison",
                "legendFormat": "Performance Data",
                "refId": "A"
              }
            ],
            "transformations": [
              {
                "id": "organize",
                "options": {
                  "excludeByName": {},
                  "indexByName": {},
                  "renameByName": {}
                }
              }
            ],
            "fieldConfig": {
              "defaults": {
                "custom": {
                  "displayMode": "color-background",
                  "align": "center"
                }
              }
            }
          }
        ],
        "time": {
          "from": "now-6h",
          "to": "now"
        },
        "refresh": "30s",
        "schemaVersion": 27,
        "version": 1
      }
