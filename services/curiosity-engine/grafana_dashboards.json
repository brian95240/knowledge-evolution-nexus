                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 0.8},
                    {"color": "green", "value": 0.9}
                  ]
                }
              }
            }
          },
          {
            "id": 7,
            "title": "üöÄ System Health Status",
            "type": "stat",
            "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
            "targets": [
              {
                "expr": "ken_system_health_status",
                "legendFormat": "Health Status",
                "refId": "A"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "mappings": [
                  {"options": {"0": {"text": "Healthy", "color": "green"}}, "type": "value"},
